      "cell_type": "code",
      "source": [
        "# Query Processing and Retrieval:\n",
        "# We use FAISS to search through the embeddings and retrieve relevant segments for the query."
      ],
      "metadata": {
        "id": "LxuJiW9FP21_"
      },
      "execution_count": 18,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# Initialize FAISS index\n",
        "def build_faiss_index(embeddings):\n",
        "    d = embeddings.shape[1]\n",
        "    index = faiss.IndexFlatL2(d)  # L2 distance for similarity\n",
        "    index.add(embeddings)\n",
        "    return index\n",
        "\n",
        "# Function to retrieve relevant context from the document\n",
        "def retrieve_context(query, segments, embeddings, index, top_k=3):\n",
        "    query_embedding = embedder.encode([query])[0]\n",
        "    _, indices = index.search(np.array([query_embedding]), top_k)\n",
        "    return [segments[i] for i in indices[0]]\n"
      ],
      "metadata": {
        "id": "Rjtu0fYVP6rx"
      },
      "execution_count": 16,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "#Cohere API for Answer Generation:\n",
        "#Generate coherent answers using the Cohere Api key"
      ],
      "metadata": {
        "id": "_H4gQ-WeP62W"
      },
      "execution_count": 17,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "import cohere\n",
        "\n",
        "# Initialize Cohere API\n",
        "cohere_client = cohere.Client('I0AVHPom94sqkUmOw2voYrr0DuyFNFZbCD9QuMPH')\n",
        "\n",
        "# Function to generate answer based on query and context\n",
        "def generate_answer(query, context):\n",
        "    combined_context = \" \".join(context)\n",
        "    response = cohere_client.generate(\n",
        "        model='command-xlarge-nightly',\n",
        "        prompt=f\"Question: {query}\\n\\nContext: {combined_context}\\n\\nAnswer:\",\n",
        "        max_tokens=100,\n",
        "        temperature=0.5,\n",
        "        k=0,\n",
        "        stop_sequences=[\"\\n\"]\n",
        "    )\n",
        "    return response.generations[0].text.strip()\n"
      ],
      "metadata": {
        "id": "4no8Td5nQGOR"
      },
      "execution_count": 19,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "#Frontend Interface using Streamlit or Gradio\n",
        "# Streamlit Interface:\n",
        "# The following code provides an interactive interface to upload documents and ask questions"
      ],
      "metadata": {
        "id": "nGqKgKTRQhwP"
      },
      "execution_count": 20,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "import streamlit as st\n",
        "\n",
        "# Streamlit interface\n",
        "st.title(\"Interactive QA Bot\")\n",
        "\n",
        "# File upload\n",
        "uploaded_file = st.file_uploader(\"Upload a PDF document\", type=\"pdf\")\n",
        "\n",
        "if uploaded_file:\n",
        "    text = extract_text_from_pdf(uploaded_file)\n",
        "    st.write(\"Document uploaded successfully!\")\n",
        "\n",
        "    # Generate embeddings for the document\n",
        "    segments, embeddings = generate_embeddings(text)\n",
        "    index = build_faiss_index(np.array(embeddings))\n",
        "\n",
        "    # Query input\n",
        "    query = st.text_input(\"Ask a question based on the document\")\n",
        "\n",
        "    if query:\n",
        "        # Retrieve relevant context\n",
        "        context = retrieve_context(query, segments, embeddings, index)\n",
        "\n",
        "        # Display retrieved segments\n",
        "        st.write(\"Retrieved Document Segments:\")\n",
        "        for segment in context:\n",
        "            st.write(segment)\n",
        "\n",
        "        # Generate answer\n",
        "        answer = generate_answer(query, context)\n",
        "        st.write(f\"Answer: {answer}\")\n"
      ],
      "metadata": {
