        "#Backend Logic for QA Bot\n",
        "#We will modify the backend from Part 1 to handle PDF files, extract text, and store embeddings for efficient query retrieval."
      ],
      "metadata": {
        "id": "CzrSqKHrKIc4"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# PDF Processing:\n",
        "#Extract text from the uploaded PDF document."
      ],
      "metadata": {
        "id": "hDFpg016KIfQ"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "import os\n",
        "import PyPDF2\n",
        "\n",
        "# Function to extract text from PDF\n",
        "def extract_text_from_pdf(file_path):\n",
        "    with open(file_path, \"rb\") as file:\n",
        "        reader = PyPDF2.PdfReader(file)\n",
        "        text = \"\"\n",
        "        for page in reader.pages:\n",
        "            text += page.extract_text() or \"\"  # Extract text from each page\n",
        "    return text\n",
        "\n",
        "# Define the PDF path\n",
        "pdf_path = r\"C:\\Users\\rites\\OneDrive\\Desktop\\Autonomous Navigation Robot[1] (1).pdf\"\n",
        "\n",
        "# Check if the file exists\n",
        "if os.path.exists(pdf_path):\n",
        "    extracted_text = extract_text_from_pdf(pdf_path)\n",
        "    print(extracted_text)\n",
        "\n",
        "# If the file does not exist, no else statement is needed.\n",
        "\n"
      ],
      "metadata": {
        "id": "6pHWkj5yKIiz"
      },
      "execution_count": 14,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# Embedding Generation:\n",
        "#We’ll use sentence-transformers to create embeddings for each segment of the document#\n",
        "from sentence_transformers import SentenceTransformer\n",
        "import numpy as np\n",
        "import faiss\n",
        "\n",
        "# Load the embedding model\n",
        "embedder = SentenceTransformer('all-MiniLM-L6-v2')\n",
        "\n",
        "# Function to generate document embeddings\n",
        "def generate_embeddings(text):\n",
        "    segments = text.split(\"\\n\\n\")  # Split the document into segments\n",
        "    embeddings = embedder.encode(segments)\n",
        "    return segments, embeddings\n"
      ],
